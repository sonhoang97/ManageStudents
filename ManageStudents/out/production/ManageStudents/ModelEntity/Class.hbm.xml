<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="managestudents" name="ModelEntity.Class" table="class">
        <id name="id" type="string">
            <column length="45" name="Id"/>
            <generator class="assigned"/>
        </id>
        <property name="className" type="string">
            <column length="45" name="ClassName"/>
        </property>
        <set inverse="true" name="Students" >
            <key>
                <column length="45" name="Id" not-null="false"/>
            </key>
            <one-to-many class="ModelEntity.Student"/>
        </set>
        <set inverse="true" name="Schedules" >
            <key>
                <column length="45" name="Id" not-null="false"/>
            </key>
            <one-to-many class="ModelEntity.Schedule"/>
        </set>
    </class>
</hibernate-mapping>