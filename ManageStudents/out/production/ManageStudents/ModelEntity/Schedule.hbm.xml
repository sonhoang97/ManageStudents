<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="managestudents" name="ModelEntity.Schedule" table="schedule">
        <id name="id" type="int">
            <column name="Id"/>
            <generator class="assigned"/>
        </id>
        <property name="room" type="string">
            <column length="45" name="Room"/>
        </property>
        <many-to-one class="ModelEntity.Class" name="classSch" fetch="select">
            <column length="45" name="ClassId" not-null="false"/>
        </many-to-one>
        <many-to-one class="ModelEntity.Subject" name="subjectSch" fetch="select">
            <column length="45" name="SubjectId" not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>